    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "SEV",
            "cargo": {
                "args": [
                    "build",
                    "--bin=dbs-cli",
                    "--package=dbs-cli"
                ],
                "filter": {
                    "name": "dbs-cli",
                    "kind": "bin"
                }
            },
            "args": [
                "--mem-size", "2048",
                "--log-file", "${workspaceFolder}/sev_test/dbs-cli.log",
                "--log-level", "trace",
                // "--serial-path", "${workspaceFolder}/sev_test/tmp.serial.sock",
                "--kernel-path", "${workspaceFolder}/sev_test/bzImage",
                "--firmware-path", "${workspaceFolder}/sev_test/final-wzy-int3.bin",
                "--rootfs", "${workspaceFolder}/sev_test/rootfs.alpine",
                "--boot-args", "console=ttyS0 root=/dev/vda1",
                "--vcpu", "1",
                "--vsock", "${workspaceFolder}/sev_test/tmp.vsock.sock",
                "create"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "clean sock (SEV)"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "普通 VM",
            "cargo": {
                "args": [
                    "build",
                    "--bin=dbs-cli",
                    "--package=dbs-cli"
                ],
                "filter": {
                    "name": "dbs-cli",
                    "kind": "bin"
                }
            },
            "args": [
                "--log-file", "${workspaceFolder}/sev_test/dbs-cli.log",
                "--log-level", "debug",
                // "--serial-path", "${workspaceFolder}/sev_test/tmp.serial.sock",
                "--kernel-path", "${workspaceFolder}/sev_test/vmlinux-daily-20220912-231412",
                // "--firmware-path", "${workspaceFolder}/sev_test/final-pe.bin",
                "--rootfs", "${workspaceFolder}/sev_test/rootfs.alpine",
                "--boot-args", "root=/dev/sda1 console=ttyS0",
                "--vsock", "${workspaceFolder}/sev_test/tmp.vsock.sock",
                "create"
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "clean sock (SEV)"
        },
    ]
}
